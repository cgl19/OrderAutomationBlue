<!DOCTYPE html>
<html>
<head>
<title>Form</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
</head>
<body style="margin: 50px;">
<div class="container">
<form action="/submit" method="post">
  <div class="mb-3 d-flex align-items-center">
    <label for="quantity" class="form-label me-3">Quantity</label>
    <input type="text" name="quantity" class="form-control" id="quantity" placeholder="Quantity">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="order_date" class="form-label me-3">Order date</label>
    <input type="date" name="order_date" class="form-control" id="order_date" placeholder="Order date">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="price" class="form-label me-3">Price</label>
    <input type="text" name="price" class="form-control" id="price" placeholder="Price">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="tax" class="form-label me-3">Tax</label>
    <input type="text" name="tax" class="form-control" id="tax" placeholder="Tax">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="shipping" class="form-label me-3">Shipping</label>
    <input type="text" name="shipping" class="form-control" id="shipping" placeholder="Shipping">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="amazon_fees" class="form-label me-3">Amazon fees</label>
    <input type="text" name="amazon_fees" class="form-control" id="amazon_fees" placeholder="Amazon fees">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="marketplace_facilitator_tax" class="form-label me-3">Marketplace facilitator tax</label>
    <input type="text" name="marketplace_facilitator_tax" class="form-control" id="marketplace_facilitator_tax" placeholder="Marketplace facilitator tax">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="your_earnings" class="form-label me-3">Your earnings</label>
    <input type="text" name="your_earnings" class="form-control" id="your_earnings" placeholder="Your earnings">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="cost" class="form-label me-3">Cost</label>
    <input type="text" name="cost" class="form-control" id="cost" placeholder="Cost">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="freight_cost" class="form-label me-3">Freight cost</label>
    <input type="text" name="freight_cost" class="form-control" id="freight_cost" placeholder="Freight cost">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="handling_misc_fee" class="form-label me-3">Handling/misc. fee</label>
    <input type="text" name="handling_misc_fee" class="form-control" id="handling_misc_fee" placeholder="Handling/misc. fee">
  </div>
  <div class="mb-3 d-flex align-items-center">
    <label for="gmd" class="form-label me-3">GMD</label>
    <input type="text" name="gmd" class="form-control" id="gmd" placeholder="GMD">
  </div>
  
  <!-- Add other fields similarly... -->
  
  <div class="mb-3 d-flex align-items-center justify-content-between">
    <label for="freightRules" class="form-label me-3">Sku Column</label>
    <div>
      <p style="display: inline-block;">Add new Sku</p>
      <a href="javascript:void(0)" id="addFrightRuleBtn" class="btn btn-primary">Add</a>
    </div>
  </div>
  <div id="skuDiv"></div>

  <!-- Submit Button -->
  <input type="submit" value="Submit" class="btn btn-primary">
</form>
</div>

</body>
</html>