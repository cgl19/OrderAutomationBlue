    <script>
      $(document).ready(function () {
        // Listen for changes in the "Processed Account" select element
        $("#processedAccount").on("change", function () {
          const selectedOption = $(this).val();

          if (selectedOption === "Other") {
            $("#otherAccountRow").show();
          } else {
            document.getElementByI('otherAccountRow').value = '';
            $("#otherAccountRow").hide();

          }
        });
      });
    </script>
